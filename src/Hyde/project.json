{
   "title": "TechSmith.Hyde",
   "version": "8.4.2-*",

   "buildOptions": {
      "warningsAsErrors": true
   },

   "dependencies": {
      "Microsoft.Data.Edm": "5.7.0",
      "Microsoft.Data.OData": "5.7.0",
      "Microsoft.Data.Services.Client": "5.7.0",
      "NETStandard.Library": "1.6.0",
      "Newtonsoft.Json": "9.0.1",
      "System.Spatial": "5.7.0",
      "WindowsAzure.Storage": "7.1.3-preview"
   },

   "frameworks": {
      "net45": {
      },
      "netstandard1.6": {
         "imports": [
            "dnxcore50",
            // import of portable-net451+win8 is needed to reference System.Runtime.InteropServices.PInvoke
            "portable-net451+win8"
         ],
         "dependencies": {
            "System.Dynamic.Runtime": "4.0.11",
            "System.Linq.Expressions": "4.1.0",
            "System.Runtime": "4.1.0",
            "System.Runtime.Extensions": "4.1.0",
            // System.Runtime.InteropServices.PInvoke is needed to allow DateTimeOffset to parse without blowing up. Hopefully this is fixed in the framework soon
            "System.Runtime.InteropServices.PInvoke": "4.0.0-rc2-24027",
            "System.Security.Cryptography.Algorithms": "4.2.0"
         }
      }
   }
}
